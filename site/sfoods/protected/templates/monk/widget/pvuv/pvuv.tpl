<div class="pvuv">
    <div class="widget">
        <div class="main">
            <div class="yesterday_value">
                <div class="intro_main">PV</div>
                <div class="value" id="pv"></div>
            </div>
            <div class="yesterday_rate">
                <div class="intro_sub">同比变化</div>
                <div class="rate">
                    <span id="pv_rate"></span>
                    <span id="pv_rate_date" class="widget-date"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="intro intro_main">PV波动</div>
            <div id="pv_wave" rel="bar" class="wave"></div>
            <div class="wave_date"></div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="yesterday_value">
                <div class="intro_main">UV</div>
                <div class="value" id="uv"></div>
            </div>
            <div class="yesterday_rate">
                <div class="intro_sub">同比变化</div>
                <div class="rate">
                    <span id="uv_rate"></span>
                    <span id="uv_rate_date" class="widget-date"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="intro intro_main">UV波动</div>
            <div id="uv_wave" rel="bar" class="wave"></div>
            <div class="wave_date"></div>
        </div>
    </div>
    <div class="clear"></div>
</div>