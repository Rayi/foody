<div class="clickcount">
    <div class="widget">
        <div class="main">
            <div class="yesterday_value">
                <div class="intro_main">点击量</div>
                <div class="value" id="totalcount"></div>
            </div>
            <div class="yesterday_rate">
                <div class="intro_sub">同比变化</div>
                <div class="rate">
                    <span id="totalcount_rate"></span>
                    <span id="totalcount_rate_date" class="widget-date"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="intro intro_main">点击量波动</div>
            <div id="totalcount_wave" rel="bar" class="wave"></div>
            <div class="wave_date"></div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="yesterday_value">
                <div class="intro_main">人均点击</div>
                <div class="value" id="percount"></div>
            </div>
            <div class="yesterday_rate">
                <div class="intro_sub">同比变化</div>
                <div class="rate">
                    <span id="percount_rate"></span>
                    <span id="percount_rate_date" class="widget-date"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="widget">
        <div class="main">
            <div class="intro intro_main">人均点击波动</div>
            <div id="percount_wave" rel="bar" class="wave"></div>
            <div class="wave_date"></div>
        </div>
    </div>
    <div class="clear"></div>
</div>